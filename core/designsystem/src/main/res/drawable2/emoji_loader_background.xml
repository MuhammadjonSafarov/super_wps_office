<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#00c4cc" android:endColor="#6f00ff" android:angle="0" android:type="linear"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/emoji_loader_background_transparent" android:endColor="@color/emoji_loader_background" android:angle="270" android:type="linear"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/emoji_loader_background_mask"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/emoji_loader_background_transparent" android:endColor="@color/emoji_loader_background" android:angle="270" android:type="linear" android:centerColor="@color/emoji_loader_background_transparent"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/emoji_loader_background" android:endColor="@color/emoji_loader_background_transparent" android:angle="0" android:type="radial" android:centerX="50%" android:centerY="100%" android:gradientRadius="150%"/>
        </shape>
    </item>
</layer-list>
